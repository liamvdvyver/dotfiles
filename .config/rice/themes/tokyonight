! palette

#define ansiBlack         #363b54
#define ansiRed           #f7768e
#define ansiGreen         #9ece6a
#define ansiYellow        #e0af68
#define ansiBlue          #7aa2f7
#define ansiMagenta       #bb9af7
#define ansiCyan          #7dcfff
#define ansiWhite         #a9b1d6
#define ansiBrightBlack   #363b54
#define ansiBrightRed     #f7768e
#define ansiBrightGreen   #9ece6a
#define ansiBrightYellow  #e0af68
#define ansiBrightBlue    #7aa2f7
#define ansiBrightMagenta #bb9af7
#define ansiBrightCyan    #7dcfff
#define ansiBrightWhite   #c0caf5
#define bground           #1a1b26
#define selection         #232433
#define comment           #444b6a

! ANSI

! black
*color0:  ansiBlack
*color8:  ansiBrightBlack
! red
*color1:  ansiRed
*color9:  ansiBrightRed
! green
*color2:  ansiGreen
*color10: ansiBrightGreen
! yellow
*color3:  ansiYellow
*color11: ansiBrightYellow
! blue
*color4:  ansiBlue
*color12: ansiBrightBlue
! magenta
*color5:  ansiMagenta
*color13: ansiBrightMagenta
! cyan
*color6:  ansiCyan
*color14: ansiBrightCyan
! white
*color7:  ansiWhite
*color15: ansiBrightWhite

! terminal

*background:  bground
*foreground:  ansiBrightWhite
*cursorColor: ansiBrightWhite

! ricing

rice.foreground.alt:      comment
rice.foreground.focused:  ansiBrightWhite
rice.foreground.active:   ansiBrightWhite
rice.foreground.inactive: ansiBrightWhite
rice.background.focused:  comment
rice.background.active:   selection
rice.background.inactive: bground
rice*urgent:              ansiBrightRed
rice.border.focused:      comment
rice.border.unfocused:    selection
rice.indicator.focused:   ansiWhite

rice.nvim.theme:      tokyonight
rice.nvim.background: dark
rice.theme:           tokyonight
rice.background:      dark
rice.doom.theme:      doom-tokyo-night
